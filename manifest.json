{
    "manifest_version": 2,
    "name": "MicroQueue", 
    "description": "a MicroQueue", 
    "version": "1.0.0",
    "background":{
        "content_scripts":["popup.js","jquery-3.3.1.min.js"]
    }, 
    "icons": {
        "128":"icon_128.png"
    }, 
    "browser_action":{
        "default_icon": "icon_128.png",
        "default_popup": "popup.html"
    }, 
    "permissions":["activeTab"]
}